            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ups_name",
            "in": "path",
            "description": "UPS name",
            "required": true,
            "allowEmptyValue": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "security": [
          {
            "ApiToken": []
          }
        ],
        "tags": [
