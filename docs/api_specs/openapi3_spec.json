          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CommandRequest"
              }
            }
          }
        },
        "parameters": [
          {
            "name": "namespace",
            "in": "path",
            "description": "Target namespace",
            "required": true,
            "allowEmptyValue": false,
            "example": "default",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ups_name",
            "in": "path",
            "description": "UPS name",
            "required": true,
            "allowEmptyValue": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "security": [
          {
            "ApiToken": []
          }
        ],
        "tags": [
